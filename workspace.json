{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "prefix": "qad",
      "skipTests": true,
      "tags": "type:app",
      "strict": true,
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "prefix": "qad",
      "linter": "eslint",
      "strict": true,
      "unitTestRunner": "jest"
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "displayBlock": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {}
}
