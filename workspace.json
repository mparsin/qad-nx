{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "prefix": "qad",
      "skipTests": true,
      "tags": "type:app",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "prefix": "qad",
      "strict": true
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "displayBlock": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "eqms": "apps/eqms",
    "eqms-e2e": "apps/eqms-e2e",
    "web-auth-data-access": "libs/web/auth/data-access",
    "web-auth-feature": "libs/web/auth/feature",
    "web-shared-utils": "libs/web/shared/utils",
    "web-shell-feature": "libs/web/shell/feature",
    "web-shell-ui-layout": "libs/web/shell/ui/layout"
  },
  "defaultProject": "eqms"
}
