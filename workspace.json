{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "inlineStyle": true,
      "prefix": "qad",
      "skipTests": true,
      "tags": "type:app",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "prefix": "qad",
      "strict": true
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "displayBlock": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "eqms": "apps/eqms",
    "eqms-auth-data-access": "libs/eqms/auth/data-access",
    "eqms-auth-feature": "libs/eqms/auth/feature",
    "eqms-auth-feature-e2e": "apps/eqms-auth-feature-e2e",
    "eqms-e2e": "apps/eqms-e2e",
    "eqms-feature-shell": "libs/eqms/feature-shell",
    "shared-animations": "libs/shared/animations",
    "shared-assets": "libs/shared/assets",
    "shared-directives-container": "libs/shared/directives/container",
    "shared-feature-layout": "libs/shared/feature-layout",
    "shared-ui-config-panel": "libs/shared/ui/config-panel",
    "shared-ui-footer": "libs/shared/ui/footer",
    "shared-ui-messages": "libs/shared/ui/messages",
    "shared-ui-messages-e2e": "apps/shared-ui-messages-e2e",
    "shared-ui-popover": "libs/shared/ui/popover",
    "shared-ui-sidebar": "libs/shared/ui/sidebar",
    "shared-ui-sidenav": "libs/shared/ui/sidenav",
    "shared-ui-toolbar": "libs/shared/ui/toolbar",
    "shared-utils": "libs/shared/utils"
  },
  "defaultProject": "eqms"
}
